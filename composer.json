{
    "name": "wolftotem4/jet-proxy",
    "description": "An PHP-based proxy, you can use it for web debugging purposes.",
    "license": "MIT",
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/process": "^3.4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "files": [
            "src/functions.php"            
        ],
        "psr-4": {
            "JetProxy\\": "src/JetProxy/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\JetProxy\\": "tests/Unit/JetProxy/"
        }
    }
}
